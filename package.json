{
  "name": "fasttiers",
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "server",
    "discord-bot"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:bot": "cd discord-bot && npm run dev",
    "build": "vite build",
    "build:server": "cd server && npm install",
    "build:bot": "cd discord-bot && npm install",
    "start": "cd server && npm start",
    "start:bot": "cd discord-bot && npm start",
    "install:all": "npm install && cd server && npm install && cd ../discord-bot && npm install",
    "db:init": "cd server && node init-db.js",
    "db:reset": "cd server && rm -f data/fasttiers.db data/fasttiers.db-shm data/fasttiers.db-wal && npm run db:init",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.app.json"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
